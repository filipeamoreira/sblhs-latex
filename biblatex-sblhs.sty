 \usepackage[backend=biber,babel=other,language=auto]{biblatex-chicago}


%% Footnote Separation
\setlength{\footnotesep}{18pt}

% Fixes footnote indents to .5in and makes number regular size
\renewcommand\@makefntext[1]{\hskip.5in\@thefnmark.\space#1}
